{
  "name": "pcileech-docs",
  "version": "1.0.0",
  "description": "PCILeech Firmware Generator Documentation Site",
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250810.0",
    "typescript": "^5.9.2",
    "wrangler": "3.84.1"
  },
  "private": true,
  "scripts": {
    "build:docs": "cd .. && python3 -m mkdocs build",
    "start": "wrangler dev",
